{
      "builds": [
            {
                  "src": "index.js",
                  "use": "@vercel/node"
            }
      ],
      "headers": [
            {
                  "source": "/(.*)",
                  "headers": [
                        {
                              "key": "Access-Control-Allow-Credentials",
                              "value": "true"
                        },
                        {
                              "key": "Access-Control-Allow-Origin",
                              "value": "https://ecommerce-fawn-pi-63.vercel.app"
                        },
                        {
                              "key": "Access-Control-Allow-Methods",
                              "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
                        },
                        {
                              "key": "Access-Control-Allow-Headers",
                              "value": "Access-Control-Allow-Origin, X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
                        }
                  ]
            }
      ],

      "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]
}
